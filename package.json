{
  "name": "jskos-proxy",
  "version": "0.2.0",
  "description": "HTTP proxy to serve JSKOS objects",
  "type": "module",
  "exports": "./server.js",
  "scripts": {
    "dev": "nodemon --watch .env --watch lib/ --watch '*.js' server.js",
    "test": "NODE_ENV=test mocha --exit",
    "lint": "eslint --ignore-path .gitignore --ext .js,.vue .",
    "fix": "npm run lint -- --fix",
    "build": "vite build",
    "start": "NODE_ENV=production node server.js",
    "release": "git checkout dev && git pull && npm test && npm version $SEMVER && git push && git checkout main && git merge dev && git push --follow-tags && git checkout dev",
    "release:patch": "SEMVER=patch npm run release",
    "release:minor": "SEMVER=minor npm run release",
    "release:major": "SEMVER=major npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbv/jskos-proxy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbv/jskos-proxy/issues"
  },
  "homepage": "https://github.com/gbv/jskos-proxy#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "axios": "^1.6.7",
    "cocoda-sdk": "^3.4.8",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-async-errors": "^3.1.1",
    "jskos-tools": "^1.0.35",
    "jskos-vue": "^0.2.9",
    "jsonld": "^8.3.2",
    "nodemon": "^3.1.0",
    "portfinder": "^1.0.32",
    "rdflib": "^2.2.33",
    "serve-static": "^1.15.0",
    "vite": "^5.1.5",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "eslint": "^8.57.0",
    "eslint-config-gbv": "1.0.5",
    "eslint-plugin-vue": "^9.22.0",
    "mocha": "^10.3.0",
    "mocha-eslint": "^7.0.0"
  }
}
